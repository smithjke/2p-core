{
  "name": "2p-core",
  "version": "0.2.22",
  "description": "",
  "main": "dist/index.ts",
  "types": "dist/index.d.ts",
  "typesVersions": {
    ">=4.0": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./dog": {
      "types": "./dist/dog/index.d.ts",
      "node": "./dist/dog/index.js",
      "require": "./dist/dog/index.js"
    }
  },
  "files": [
    "dist",
    "dist/**/!(*.tsbuildinfo)"
  ],
  "scripts": {
    "prepare": "echo ABOBA prepare >> aboba.txt",
    "preinstall": "echo ABOBA preinstall >> aboba.txt && tsc",
    "prepack": "echo ABOBA prepack >> aboba.txt",
    "prepublishOnly": "echo ABOBA prepublishOnly >> aboba.txt",
    "prepublish": "echo ABOBA prepublish >> aboba.txt",
    "preversion": "echo ABOBA preversion >> aboba.txt",
    "prerestart": "echo ABOBA prerestart >> aboba.txt",
    "postpack": "echo ABOBA postpack >> aboba.txt",
    "postpublish": "echo ABOBA postpublish >> aboba.txt",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "up:patch": "yarn version --patch && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smithjke/2p-core.git"
  },
  "author": "smithjke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smithjke/2p-core/issues"
  },
  "homepage": "https://github.com/smithjke/2p-core#readme",
  "devDependencies": {
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "rxjs": "^7.5.7"
  }
}
